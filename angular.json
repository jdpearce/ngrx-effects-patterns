{
  "version": 1,
  "projects": {
    "todos": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/todos",
      "sourceRoot": "apps/todos/src",
      "prefix": "ngrx-effects-patterns",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/todos",
            "index": "apps/todos/src/index.html",
            "main": "apps/todos/src/main.ts",
            "polyfills": "apps/todos/src/polyfills.ts",
            "tsConfig": "apps/todos/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/todos/src/favicon.ico",
              "apps/todos/src/assets"
            ],
            "styles": [
              "apps/todos/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/todos/src/environments/environment.ts",
                  "with": "apps/todos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "todos:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "todos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todos:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/todos/tsconfig.app.json",
              "apps/todos/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/todos/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/todos/jest.config.js",
            "tsConfig": "apps/todos/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/todos/src/test-setup.ts"
          }
        }
      }
    },
    "todos-e2e": {
      "root": "apps/todos-e2e",
      "sourceRoot": "apps/todos-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/todos-e2e/cypress.json",
            "tsConfig": "apps/todos-e2e/tsconfig.e2e.json",
            "devServerTarget": "todos:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "todos:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/todos-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/todos-e2e/**"
            ]
          }
        }
      }
    },
    "todos-feature-ngrx-testing": {
      "projectType": "library",
      "root": "libs/todos/feature/ngrx-testing",
      "sourceRoot": "libs/todos/feature/ngrx-testing/src",
      "prefix": "ngrx-effects-patterns",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/todos/feature/ngrx-testing/tsconfig.lib.json",
              "libs/todos/feature/ngrx-testing/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/todos/feature/ngrx-testing/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/todos/feature/ngrx-testing/jest.config.js",
            "tsConfig": "libs/todos/feature/ngrx-testing/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/todos/feature/ngrx-testing/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "todos"
}
